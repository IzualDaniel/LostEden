<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bck">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton"
        android:layout_x="75dp"
        android:layout_y="48dp"
        android:src="@drawable/d1"
        android:background="#02ffffff"
        android:layout_marginTop="60dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="52dp" />

    <ImageButton
        android:layout_width="47dp"
        android:layout_height="49dp"
        android:id="@+id/imageButton2"
        android:layout_x="145dp"
        android:layout_y="58dp"
        android:src="@drawable/d2"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="52dp"
        android:layout_height="53dp"
        android:id="@+id/imageButton3"
        android:layout_x="231dp"
        android:layout_y="48dp"
        android:src="@drawable/d3"
        android:layout_alignTop="@+id/imageButton"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="52dp" />

    <ImageButton
        android:layout_width="54dp"
        android:layout_height="55dp"
        android:id="@+id/imageButton4"
        android:layout_x="76dp"
        android:layout_y="116dp"
        android:src="@drawable/d4"
        android:layout_alignTop="@+id/imageButton5"
        android:layout_alignStart="@+id/imageButton" />

    <ImageButton
        android:layout_width="56dp"
        android:layout_height="57dp"
        android:id="@+id/imageButton5"
        android:layout_x="157dp"
        android:layout_y="131dp"
        android:src="@drawable/d5"
        android:layout_marginTop="48dp"
        android:layout_below="@+id/imageButton"
        android:layout_alignEnd="@+id/imageButton3" />

    <ImageButton
        android:layout_width="59dp"
        android:layout_height="60dp"
        android:id="@+id/imageButton6"
        android:layout_x="244dp"
        android:layout_y="127dp"
        android:src="@drawable/d6"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_alignEnd="@+id/imageButton2" />

</RelativeLayout>