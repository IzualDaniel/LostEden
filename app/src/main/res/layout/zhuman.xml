<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bck">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton8"
        android:src="@drawable/h1"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="44dp"
        android:layout_marginTop="69dp"
        android:background="#000000"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton9"
        android:src="@drawable/h2"
        android:background="#000000"
        android:layout_alignTop="@+id/imageButton8"
        android:layout_centerHorizontal="true"
        />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton10"
        android:src="@drawable/h3"
        android:background="#000000"
        android:layout_alignTop="@+id/imageButton9"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="44dp"
        />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton11"
        android:layout_marginTop="50dp"
        android:src="@drawable/h4"
        android:layout_below="@+id/imageButton8"
        android:layout_alignStart="@+id/imageButton8"
        android:background="#000000"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton12"
        android:src="@drawable/h5"
        android:layout_alignTop="@+id/imageButton11"
        android:layout_alignStart="@+id/imageButton9"
        android:background="#000000"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton13"
        android:src="@drawable/h6"
        android:layout_alignTop="@+id/imageButton12"
        android:layout_alignStart="@+id/imageButton10"
        android:background="#000000"/>
</RelativeLayout>